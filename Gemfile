source "https://rubygems.org"

# Dependency information for github pages can be found here: https://pages.github.com/versions.json
# As of 2026-01-15, we have `{ ..., "github-pages": "232", "ruby": "3.3.4", ...}`.
# We pin the github pages version below,
# and the ruby version in the `.ruby-version` file.
# We use `chruby` in `~/.profile` to set the ruby version for the command line.

gem "github-pages", "~> 232", group: :jekyll_plugins

group :jekyll_plugins do
  gem "jekyll-feed"
end

gem "webrick"
